{
  "name": "tdf",
  "version": "0.0.1",
  "scripts": {
    "lint": "eslint src/ --ext .js",
    "start": "webpack serve --progress",
    "build": "rm -rf build; webpack --config ./config/webpack.config.prod.js --progress",
    "deploy": "gh-pages -d build",
    "prepush": "npm run build; npm run deploy"
  },
  "husky": {
    "hooks": {
      "commit-msg": "node ./node_modules/fit-commit-js/lib/hook.js .git/COMMIT_EDITMSG",
      "pre-commit": "npm run lint",
      "pre-push": "npm run prepush"
    }
  },
  "prettier": {
    "semi": false,
    "trailingComma": "all",
    "bracketSpacing": false,
    "printWidth": 100
  },
  "dependencies": {
    "ramda": "^0.27.1"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^6.2.0",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "fit-commit-js": "^0.3.2",
    "gh-pages": "^3.2.3",
    "husky": "^4.3.8",
    "mini-css-extract-plugin": "^2.2.0",
    "nunjucks": "^3.2.3",
    "nunjucks-webpack-plugin": "^5.0.0",
    "prettier": "^2.3.2",
    "style-loader": "^3.2.1",
    "webpack": "^5.48.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  }
}
