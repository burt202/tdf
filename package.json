{
  "name": "tdf",
  "version": "0.0.1",
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint --ext js,ts,tsx src/ webpack/",
    "prettier": "prettier \"**/*.ts{,x}\" --check",
    "pre-commit": "npm run lint && npm run prettier",
    "pre-push": "npm run build; npm run deploy",
    "dev": "webpack serve --config ./webpack/config.dev.ts",
    "build": "rm -rf build; webpack --progress --config ./webpack/config.prod.ts",
    "deploy": "gh-pages -d build"
  },
  "devDependencies": {
    "@types/node": "^16.18.38",
    "@types/ramda": "^0.29.3",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "2.26.0",
    "fit-commit-js": "^0.3.2",
    "gh-pages": "^4.0.0",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^8.0.3",
    "mini-css-extract-plugin": "^2.7.6",
    "prettier": "^2.8.8",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "typescript": "5.0.4",
    "webpack": "^5.88.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.9.0"
  },
  "dependencies": {
    "ramda": "^0.29.0"
  }
}
